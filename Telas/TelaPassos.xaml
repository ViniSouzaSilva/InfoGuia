<Window x:Class="AmbiTroubleShooting.Telas.TelaPassos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AmbiTroubleShooting.Telas"
        mc:Ignorable="d"
        Title="TelaPassos" Height="470" Width="805" ResizeMode="NoResize" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel x:Name="Stack1" Orientation="Vertical" Margin="10,15,0,50" HorizontalAlignment="Left" Width="350" />
        <StackPanel x:Name="Stack2" Orientation="Vertical" Margin="390,15,0,50" />
        <Button Content="Voltar" Click="Button_Click" HorizontalAlignment="Left" Margin="20,0,0,5" VerticalAlignment="Bottom" Width="110" Height="40" BorderBrush="#FF4098F0" Background="#FF4098F0" Foreground="#FFFBFBFB"/>
        <Button Content="Finalizar" Click="Button_Click_1" HorizontalAlignment="Right" Margin="0,0,25,5" VerticalAlignment="Bottom" Width="110" Height="40" BorderBrush="#FFFD4747" Foreground="White" Background="#FFF15050"/>



    </Grid>
</Window>  
